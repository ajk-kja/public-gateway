# Public Gateway Environment Variables
# Copy this to .env and fill in your values

# Tailscale IP for direct access binding
# Set this to your VPS Tailscale IP (e.g., 100.x.x.x)
# Leave empty or 0.0.0.0 to bind to all interfaces
TAILSCALE_IP=

# Cloudflare Tunnel Token
# This token connects to Cloudflare's network for public access
# Generate via: cloudflared tunnel token <tunnel-name>
CLOUDFLARE_TUNNEL_TOKEN=
